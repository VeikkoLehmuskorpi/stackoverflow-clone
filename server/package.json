{
  "name": "stackoverflow-clone-server",
  "version": "0.1.0",
  "description": "Stackoverflow clone backend",
  "main": "dist/index.js",
  "repository": "git@github.com:VeikkoLehmuskorpi/stackoverflow-clone.git",
  "author": "Veikko Lehmuskorpi",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
    "typeorm": "node ./node_modules/typeorm/cli.js",
    "typeorm:migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "typeorm:migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "typeorm:migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.4",
    "class-validator": "^0.12.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "apollo-server-express": "^2.17.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "helmet": "^4.1.0",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.0.0",
    "typeorm": "^0.2.25"
  }
}
